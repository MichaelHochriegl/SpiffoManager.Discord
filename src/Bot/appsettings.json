{
  "ApplicationSettings": {
    "UsedConnectionString": "Production"
  },
  "ConnectionStrings": {
    "Production": "Data Source=./SpiffoManager.db;Version=3;"
  },
  "DiscordBotSettings": {
    "Token": "",
    "GuildId": 0,
    "GameserverRoleId": 0
  },
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": ["FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId"],
    "WriteTo": [
      {"Name":  "Console"},
      { "Name": "File", "Args": { "path": "log.txt", "rollingInterval": "Day" } }
    ]
  }
}
